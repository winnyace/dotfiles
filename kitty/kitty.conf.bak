# Winny's kitty config

#Basic
editor /usr/bin/nvim
allow_remote_control yes
listen_on unix:/tmp/kitty
background_opacity 1

#Fonts
font_family UbuntuMono Nerd Font 
font_size 18

# Cursor
cursor_shape block
cursor_blink_interval 0

# BEGIN_KITTY_THEME
# Catppuccin-Mocha
include current-theme.conf
# END_KITTY_THEME
